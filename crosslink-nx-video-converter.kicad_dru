(version 1)

(rule "ddr3_inner"
	(constraint track_width (opt 0.162mm))
	(constraint clearance (opt 0.6mm))
#	(constraint clearance (min 0.1mm))
	(constraint diff_pair_gap (opt 0.311mm))
	(condition "(A.NetClass == '50Ohm-se_DDR3' || A.NetClass == '90Ohm-diff_DDR3')")
	(layer inner)
)

(rule "ddr3_outer"
	(constraint track_width (opt 0.200mm))
	(constraint diff_pair_gap (opt 0.196mm))
	(constraint clearance (opt 0.6mm))
	(condition "(A.NetClass == '50Ohm-se_DDR3' || A.NetClass == '90Ohm-diff_DDR3')")
	(layer outer)
)

